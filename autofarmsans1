local active = true


script.Parent.MouseButton1Click: Connect(function()
	if active == false then
		active = true
	elseif active == true then
		active = false
	
	end
end)
while active == true  do
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(2664.38647, 205.061279, -41.7528648) 
	print("Success")
	wait(5)
end
